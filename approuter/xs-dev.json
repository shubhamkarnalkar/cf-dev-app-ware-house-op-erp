            "target": "$1",
            "service": "sap-approuter-userapi"
        },
        {
            "dependency": "uimodule",
            "authenticationType": "xsuaa"
        },
        {
            "dependency": "sap-btp-cap-warehouse-op",
            "authenticationType": "xsuaa"
        }
    ]
}